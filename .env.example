# Comma-separated list of Google email addresses allowed to log in (leave empty to allow all)
ALLOWED_EMAILS=you@example.com
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=+1xxxxxxxxxx
OPENAI_API_KEY=sk-xxxx
GOOGLE_PLACES_API_KEY=your_google_places_api_key
# Google OAuth (for MCP/Claude.ai integration) — create at console.cloud.google.com
GOOGLE_OAUTH_CLIENT_ID=your_google_oauth_client_id
GOOGLE_OAUTH_CLIENT_SECRET=your_google_oauth_client_secret
# Random secret for signing JWTs — generate with: openssl rand -hex 32
JWT_SECRET=your_jwt_secret
DATABASE_URL=postgres://user:pass@localhost:5432/call_agent
PUBLIC_URL=https://your-ngrok-url.ngrok-free.app
PORT=3000
NODE_ENV=development
